<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sulekhasurbhi.swasthya"
    android:installLocation="preferExternal">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.Diagnosis"></activity>
        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.CurrLocDetailsActivity" />
        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.CurrLocation" />
        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.TinytipsDetialsActivity" />
        <activity
            android:name="com.example.sulekhasurbhi.swasthya.Activity.DetailsGNActivity"
            android:parentActivityName="com.example.sulekhasurbhi.swasthya.Activity.HomeScreenActivity" />
        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.IntroActivity" />
        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.LoginActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.RegisterActivity" />
        <activity android:name="com.example.sulekhasurbhi.swasthya.Activity.HomeScreenActivity" />
        <activity
            android:name="com.example.sulekhasurbhi.swasthya.Activity.DetailsActivity"
            android:parentActivityName="com.example.sulekhasurbhi.swasthya.Activity.HomeScreenActivity" />
    </application>

</manifest>